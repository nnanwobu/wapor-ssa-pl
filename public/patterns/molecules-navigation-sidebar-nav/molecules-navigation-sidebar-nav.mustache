 <div class="page-sideBar-inner">
	<div class="page-sideBar-inner-content">    
		<ul class="sidebar-nav__list">

				{{# navItems }}
			
				<li class="sidebar-nav__item {{ sidebar-styleModifier }}" id="{{id}}">
				
					<a href="{{ url }}" class="sidebar-nav__link">{{ label }}</a><!--end c-primary-nav__link-->
					
					<ul class="sidebar-submenu__list" id="{{id}}">
					{{# submenu}}
					
						<li class="sidebar-submenu__listItem"><a href="{{url}}" class="sidebar-submenu__listLink">{{label}}</a> 
						</li>
					
					{{/submenu}}
					</ul>
					
					
				
				</li><!-- end c-primary-nav__list__item -->
				{{/ navItems }}

		</ul><!-- end c-primary-nav__list -->
	</div>
</div>
